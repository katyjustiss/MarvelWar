div.main-content
  h1 Assemble your team
  p Select 5 members for your team. Click image.
  div(ng-controller="CardCtrl as card")
    slick(settings="slickConfig", init-onload=true, data="card.data")
      div.carousel_wrapper(ng-repeat="c in card.data")
        a(ng-click="open(c)")
          img.cardImg(ng-src="{{c.image}}" alt="character")
          h4.caption {{c.name}}
